<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberList}} <!--머스테치 데이터가 묶음인경우 for문처럼 안의 내용이 반복됨을 인지할 것.-->
    <tr>
        <th>{{id}}</th>
        <td><a href="/members/{{id}}">{{email}}</a></td>
        <td>{{password}}</td>
    </tr>
    {{/memberList}} <!-- 답지는 /members로 되있음 -->
    </tbody>
</table>

<a href="/members/new">회원가입</a>

